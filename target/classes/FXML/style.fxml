<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<AnchorPane stylesheets="/CSS/style.css" prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="config.style_controller">
   <children>
      <TableView fx:id="StyleFX" prefHeight="400.0" prefWidth="1000.0">
        <columns>
          <TableColumn fx:id="Id_stylu" prefWidth="75.0" text="Id stylu" visible="false"/>
          <TableColumn fx:id="Nazwa_stylu" prefWidth="75.0" text="Nazwa stylu" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
       <HBox layoutY="401.0" prefHeight="75.0" prefWidth="1000.0">
           <children>
               <Button fx:id="st_add" mnemonicParsing="false" onAction="#add" prefHeight="50" prefWidth="333" HBox.hgrow="ALWAYS" />
               <Button fx:id="st_edit" mnemonicParsing="false" onAction="#edit" prefHeight="50" prefWidth="333" HBox.hgrow="ALWAYS" />
               <Button fx:id="st_del" mnemonicParsing="false" onAction="#delete" prefHeight="50" prefWidth="333"  HBox.hgrow="ALWAYS" />
           </children>
       </HBox>
   </children>
</AnchorPane>