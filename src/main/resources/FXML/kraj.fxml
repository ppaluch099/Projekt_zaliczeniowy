<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="/CSS/style.css" prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="config.kraj_controller">
   <children>
      <TableView fx:id="KrajFX" prefHeight="400.0" prefWidth="1000.0">
        <columns>
          <TableColumn fx:id="Id_kraju" prefWidth="75.0" text="Id kraju" visible="false" />
          <TableColumn fx:id="Nazwa_kraju" prefWidth="75.0" text="Nazwa kraju" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
       <HBox layoutY="401.0" prefHeight="75.0" prefWidth="1000.0">
           <children>
               <Button fx:id="kr_add" mnemonicParsing="false" onAction="#add" prefHeight="75" prefWidth="333" HBox.hgrow="ALWAYS" />
               <Button fx:id="kr_edit" mnemonicParsing="false" onAction="#edit" prefHeight="75" prefWidth="333" HBox.hgrow="ALWAYS" />
               <Button fx:id="kr_del" mnemonicParsing="false" onAction="#delete" prefHeight="75" prefWidth="333"  HBox.hgrow="ALWAYS" />
           </children>
       </HBox>
   </children>
</AnchorPane>